{
    "name": "miracode/stripe-bundle",
    "description": "Integrates stripe in Symfony project",
    "keywords": ["symfony", "stripe", "payment"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name" : "Aleksey Mirovskyi",
            "email": "mirovskyi@gmail.com"
        }
    ],
    "require": {
        "php"                          : ">=5.4.0",
        "symfony/framework-bundle"     : ">=2.4",
        "symfony/config"               : ">=2.3",
        "stripe/stripe-php"            : ">=3.0",
        "doctrine/common"              : ">=2.2"
    },
    "suggest": {
        "doctrine/orm" : "If you want to save stripe data in database"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Miracode\\StripeBundle\\": "src/"
        }
    }
}
